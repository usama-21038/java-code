//series-1+3+5+...+n
import java.util.*;
public class series_2{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter range:");
      int n=sc.nextInt();
      int sum=0;
      for(int i=1;i<=n;i=i+2){
       sum=sum+i;
       System.out.print(i+" ");
      }
      System.out.println("\nSum of series: "+sum);
        
}
}
