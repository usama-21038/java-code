import java.util.*;
public class EvenOdd{
    public static void main(String[] args){
    int n;
    Scanner input=new Scanner(System.in);
    System.out.print("Enter any number:");
    n=input.nextInt();
    if(n%2==0){
        System.out.println("Number is even!");
    }
    else
    System.out.println("Number is odd!");
    }
}
