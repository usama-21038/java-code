import java.util.*;
public class Reverse_Of_Number{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int sum=0,rem,temp,num;
System.out.print("Enter any number:");
num=sc.nextInt();
temp=num;
while(temp!=0){
    rem=temp%10;
    sum=sum*10+rem;
    temp=temp/10;
    
}System.out.print("Reverse of digit:"+sum);

        
    }
}
