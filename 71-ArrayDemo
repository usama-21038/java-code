import java.util.*;
public class ArrayDemo{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int []num=new int[5];
        for(int i=0;i<5;i++){
            System.out.print("Array["+i+"]=");
            num[i]=sc.nextInt();
        }
        int sum=0;
        for(int i=0;i<5;i++){
            sum=sum+num[i];
        }System.out.println("Sum of values:"+sum);
        
    }
}
